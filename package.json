{
    "name": "ng-onpush",
    "version": "0.0.0",
    "scripts": {
        "build:test": "rm -rf test && tsc --project test-src/tsconfig.json",
        "build:test-app": "ng build test --configuration production",
        "prepublishOnly": "exit 1",
        "pretest": "npm run build:test && npm run build:test-app",
        "pub": "packagr",
        "start": "ng serve test",
        "test": "mocha --config test/mocha.config.js",
        "watch": "ng build ng-onpush --watch --configuration development"
    },
    "private": true,
    "devDependencies": {
        "@angular-devkit/build-angular": "^14.1.2",
        "@angular/cli": "^14.1.2",
        "@angular/common": "^14.1.2",
        "@angular/compiler": "^14.1.2",
        "@angular/compiler-cli": "^14.1.2",
        "@angular/core": "^14.1.2",
        "@angular/platform-browser": "^14.1.2",
        "@angular/platform-browser-dynamic": "^14.1.2",
        "@angular/router": "^14.1.2",
        "@types/estree": "^0.0.52",
        "@types/mocha": "^9.1.1",
        "@types/node": "^16.11.7",
        "@types/serve-handler": "^6.1.1",
        "mocha": "^10.0.0",
        "ng-packagr": "^14.1.0",
        "node-path-tags": "^1.0.0",
        "npm-packagr": "^1.0.57",
        "prettier": "^2.7.1",
        "puppeteer": "^16.1.0",
        "rxjs": "^7.5.6",
        "serve-handler": "^6.1.3",
        "tslib": "^2.4.0",
        "typescript": "^4.7.4",
        "zone.js": "~0.11.8"
    }
}
